{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "monkeyc",
      "camelcase": "MonkeyC",
      "title": "Monkeyc",
      "scope": "source.monkeyc",
      "file-types": [
        "mc"
      ],
      "injection-regex": "^monkeyc$",
      "class-name": "TreeSitterMonkeyc"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "MonkeyC grammar for tree-sitter",
    "authors": [
      {
        "name": "linyun",
        "email": "liinyun.weng@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/liinyun/tree-sitter-monkeyc.git"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}